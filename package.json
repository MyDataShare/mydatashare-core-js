{
  "name": "mydatashare-core",
  "version": "3.0.0",
  "description": "Utilities for working with the MyDataShare API",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "preversion": "npm test",
    "test": "jest --silent --verbose --coverage",
    "lint": "eslint --fix src",
    "docs": "jsdoc2md -t README.hbs src/*.js src/api/*.js src/store/*.js src/store/base/*.js > README.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MyDataShare/mydatashare-core-js"
  },
  "keywords": [
    "vastuugroup",
    "mydata",
    "mydatashare",
    "authentication",
    "pkce",
    "oauth",
    "oidc",
    "openidconnect",
    "authorization"
  ],
  "author": "Vastuu Group",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jsdoc-to-markdown": "^6.0.1",
    "lint-staged": "^10.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": "eslint --cache --fix"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@openid/appauth": "^1.3.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
